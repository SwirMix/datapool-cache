igo\"\n                class=\"ma-4\"\n              >\n                <v-icon dark>\n                  mdi-plus\n                </v-icon>\n              </v-btn>\n          </v-row>\n        </v-card-text>\n        <v-divider></v-divider>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn\n            color=\"success\"\n            text\n            @click=\"dialog = false\"\n          >\n            UPDATE\n          </v-btn>\n          <v-btn\n            color=\"primary\"\n            text\n            @click=\"dialog = false\"\n          >\n            close\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n<script>\n  import {mapActions} from 'vuex'\n\n  export default {\n    props: {\n      item: {\n        type:Object,\n        default(){\n            return {}\n        }\n      }\n    },\n    data () {\n      return {\n        dialog: false,\n      }\n    },\n    methods: {\n      ...mapActions([\n         'REFRESH_CACHES'\n      ]),\n    }\n  }\n</script>"]},"metadata":{},"sourceType":"module"}